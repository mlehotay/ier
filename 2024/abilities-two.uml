@startuml
skinparam class {
    BackgroundColor White
    ArrowColor Black
    BorderColor Black
}

class "Life" as Life {
    + Metabolism
    + Growth
    + Response to stimuli
}

class "Complex System" as ComplexSys {
    + Interconnected parts
    + Emergent properties
}

class "Basic Information Processing" as BIP {
    + Sensory reception
    + Basic motor responses
    + Simple biochemical reactions (in non-neural organisms)
}

class "Reproduction" as Repro {
    + Biological process
    + Production of new organisms
}

class "Persistence" as Persist {
    + Endurance through time
    + Continuation of existence
}

class "Intelligence" as Intel {
    + Learning from experience
    + Problem-solving
    + Adaptability to changing environments
    + Memory storage and retrieval
}

class "Sentience" as Sent {
    + Perception of sensory stimuli
    + Emotional responses to stimuli
    + Awareness of internal states and external environment
}

class "Existence" as Exist {
    + Consciousness of one's own existence
    + Recognition of oneself in mirrors or reflections
}

class "Consciousness" as Consc {
    + Reflective thinking
    + Attention and focus
    + Memory and awareness of past experiences
}

class "Subjective Experiences (Qualia)" as Qualia {
    + Ability to have sensory perceptions
    + Emotional experiences
    + Unique and individualized experiences of the world
}

class "Self-Awareness" as SelfAware {
    + Awareness of one's own thoughts, feelings, and actions
}

Life --> ComplexSys : "Foundation for"
ComplexSys --> BIP : "Foundation for"
BIP --> Repro : "Foundation for"
Repro --> Persist : "Foundation for"
Persist --> Intel : "Foundation for"
Intel --> Sent : "Foundation for"
Sent --> Exist : "Foundation for"
Exist --> Consc : "Foundation for"
Consc --> Qualia : "Foundation for"
Qualia --> SelfAware : "Foundation for"

@enduml
